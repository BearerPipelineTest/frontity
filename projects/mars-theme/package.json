{
  "name": "mars-theme-project",
  "version": "1.0.0",
  "private": true,
  "description": "A simple Frontity project using mars-theme for testing purposes",
  "scripts": {
    "dev": "frontity dev",
    "dev:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity dev",
    "build": "frontity build",
    "build:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity build",
    "serve": "frontity serve",
    "serve:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity serve",
    "vercel": "npx frontity build && npx vercel --confirm --token $NOW_TOKEN",
    "vercel:alias": "npx vercel alias --token $NOW_TOKEN set mars-theme-worona.frontity.vercel.app mars-theme-$COMMIT_SHA.vercel.app"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@frontity/core": "^1.10.1",
    "@frontity/html2react": "^1.6.1",
    "@frontity/mars-theme": "^1.5.0",
    "@frontity/tiny-router": "^1.3.2",
    "@frontity/wp-source": "^1.11.1",
    "frontity": "^1.14.1",
    "with-preact": "./packages/with-preact"
  }
}
