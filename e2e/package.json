{
  "name": "e2e",
  "private": false,
  "scripts": {
    "start": "node e2e.js",
    "browserstack": "nodenv --exec 'browserstack-cypress run --sync'",
    "local:macos": "nodenv --exec './browserstack-local/macOS --key \"$BROWSERSTACK_ACCESS_KEY\"'",
    "local:macos:start": "nodenv --exec './browserstack-local/macOS --key \"$BROWSERSTACK_ACCESS_KEY\" --daemon start'",
    "local:macos:stop": "nodenv --exec './browserstack-local/macOS --key \"$BROWSERSTACK_ACCESS_KEY\" --daemon stop'",
    "local:win": "nodenv --exec './browserstack-local/win.exe --key \"$BROWSERSTACK_ACCESS_KEY\"'",
    "local:win:start": "nodenv --exec './browserstack-local/win.exe --key \"$BROWSERSTACK_ACCESS_KEY\" --daemon start'",
    "local:win:stop": "nodenv --exec './browserstack-local/win.exe --key \"$BROWSERSTACK_ACCESS_KEY\" --daemon stop'",
    "local:linux": "nodenv --exec './browserstack-local/linux --key \"$BROWSERSTACK_ACCESS_KEY\"'",
    "local:linux:start": "nodenv --exec './browserstack-local/linux --key \"$BROWSERSTACK_ACCESS_KEY\" --daemon start'",
    "local:linux:stop": "nodenv --exec './browserstack-local/linux --key \"$BROWSERSTACK_ACCESS_KEY\" --daemon stop'"
  },
  "devDependencies": {
    "@types/cypress": "^1.1.3",
    "@types/execa": "^2.0.0",
    "@types/wait-on": "^4.0.0",
    "browserstack-cypress-cli": "^1.5.2",
    "chromedriver": "^86.0.0",
    "node-env-run": "^4.0.2",
    "node-fetch": "^2.6.1",
    "selenium-side-runner": "^3.17.0",
    "cypress": "^6.2.1",
    "cypress-plugin-snapshots": "^1.4.3",
    "execa": "^4.0.3",
    "forever": "^3.0.4",
    "http-server": "^0.12.3",
    "expect": "^26.6.2",
    "minimist": "^1.2.5",
    "wait-on": "^5.2.1"
  }
}
