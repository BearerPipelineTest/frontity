{
  "extends": "../tsconfig",
  // Options recommended by Cypress.
  // https://docs.cypress.io/guides/tooling/typescript-support.html#Configure-tsconfig-json
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress"]
  },
  // Include TypeScript React files as well.
  "include": ["**/*.tsx", "**/*.ts"]
}
