{
  "auth": {
    "username": "$BROWSERSTACK_USERNAME",
    "access_key": "$BROWSERSTACK_ACCESS_KEY"
  },
  "browsers": [
    {
      "browser": "firefox",
      "os": "Windows 10",
      "versions": ["latest"]
    },
    {
      "browser": "edge",
      "os": "Windows 10",
      "versions": ["latest"]
    }
  ],
  "run_settings": {
    "cypress_config_file": "cypress.browserstack.json",
    "cypress_version": "6",
    "project_name": "frontity/frontity",
    "build_name": "Latest Browsers",
    "parallels": "1",
    "specs": [
      "integration/frontity-*/**/*.spec.js",
      "integration/frontity-*/**/*.spec.ts"
    ],
    "npm_dependencies": {
      "cypress-plugin-snapshots": "^1.4.3",
      "expect": "^26.6.2",
      "typescript": "^4.2.3",
      "execa": "^4.0.3"
    }
  },
  "connection_settings": {
    "local": true,
    "local_identifier": "Cypress-Local"
  }
}
