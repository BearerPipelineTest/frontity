{
  "name": "twentytwenty-theme-project",
  "version": "1.0.0",
  "private": true,
  "description": "A simple Frontity project using twentytwenty-theme for testing purposes",
  "scripts": {
    "dev": "frontity dev",
    "dev:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity dev",
    "build": "frontity build",
    "build:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity build",
    "serve": "frontity serve",
    "serve:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity serve",
    "vercel": "npx frontity build && npx vercel --confirm --token $NOW_TOKEN",
    "vercel:alias": "npx vercel alias --token $NOW_TOKEN set twentytwenty-theme-worona.frontity.vercel.app twentytwenty-theme-$COMMIT_SHA.vercel.app"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@frontity/core": "^1.5.0",
    "@frontity/html2react": "^1.2.0",
    "@frontity/twentytwenty-theme": "^1.0.0",
    "@frontity/tiny-router": "^1.1.0",
    "@frontity/wp-source": "^1.5.0",
    "frontity": "^1.5.0"
  }
}
